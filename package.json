{
  "name": "postcss-critical-css",
  "version": "1.0.3",
  "description": "Generate critical CSS using PostCSS",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/zgreen/postcss-critical-css"
  },
  "keywords": [
    "postcss-plugin",
    "postcss plugin",
    "postcss",
    "critical-css",
    "critical",
    "css",
    "critical css"
  ],
  "author": "Zach Green",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.13.2",
    "chalk": "^1.1.3",
    "cssnano": "^3.5.2",
    "eslint": "^3.3.1",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-flowtype": "^2.7.1",
    "eslint-plugin-import": "^1.13.0",
    "flow-bin": "^0.30.0",
    "postcss": "^5.0.5",
    "tape": "^4.6.0"
  },
  "scripts": {
    "build": "eslint src/** && npm run flow && babel src/index.js --out-file index.js",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "eslint": "eslint src/**",
    "start": "eslint src/** && npm run flow && babel src/index.js --out-file index.js --watch",
    "pretest": "./node_modules/.bin/babel-node test/preTest.js",
    "test": "npm run flow && npm run pretest && tape test"
  },
  "dependencies": {
    "chalk": "^1.1.3",
    "cssnano": "^3.7.4",
    "postcss": "^5.1.2"
  }
}
